define(["text!pages/ScrumBoard/templates/IssueView.html"],

    function(issueView) {
        return Backbone.View.extend({
            template: _.template(issueView), 
        
            initialize: function(options){
            },
                           
            render: function () {
                alert("1111");
                this.$el.html(this.template({name: this.model.get('name')}));
                return this;
            }
        })

});

